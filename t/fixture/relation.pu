@startuml

class Main {
  run()
}

class Plantuml::Parser {
  classes
  relations
  parse()
  _extract_class_strings()
  _extract_relation_strings()
}

class Plantuml::Class::Factory {
  create()
  _check_is_method()
  _check_is_variable()
}

class Plantuml::Class {
  attribute
  parents
  variables
  methods
  build()
}

class Plantuml::Class::Variable {
  name
  attribute
  build()
}

class Plantuml::Class::Method {
  name
  attribute
  build()
}

class Plantuml::Relation {
  from
  to
  name
  build()
}

Class *-- Variable
Class *-- Method
Class <-- Relation
Factory <-- Class
Factory ..|> Variable
Factory ..|> Method

@enduml
